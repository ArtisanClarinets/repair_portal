{
	"servers": {
		"sequentialthinking": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-sequential-thinking"
			],
			"type": "stdio"
		},
		"memory": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-memory"
			],
			"env": {
				"MEMORY_FILE_PATH": "${input:memory_file_path}"
			},
			"type": "stdio"
		},
		"desktop-commander": {
			"command": "npx",
			"args": [
				"-y",
				"@wonderwhy-er/desktop-commander"
			],
			"type": "stdio"
		},
		"frappe": {
			"command": "npx",
			"args": [
				"frappe-mcp-server"
			],
			"env": {
				"FRAPPE_URL": "https://erp.artisanclarinets.com",
				"FRAPPE_API_KEY": "af02116fcd5e508",
				"FRAPPE_API_SECRET": "73a7279a5b06eb6"
			},
			"type": "stdio"
		},
		"upstash-context7": {
			"args": [
				"-y",
				"@upstash/context7-mcp"
			],
			"command": "npx",
			"type": "stdio"
		},
	},
	"inputs": [
		{
			"id": "memory_file_path",
			"type": "promptString",
			"description": "Path to the memory storage file (optional)",
			"password": false
		}
	]
}