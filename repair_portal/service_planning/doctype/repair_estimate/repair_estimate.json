      "write": 1,
      "create": 1,
      "delete": 1
    },
    {
      "role": "Technician",
      "read": 1,
      "write": 1,
      "create": 1
    },
    {
      "role": "Customer",
      "read": 1,
      "create": 1,
      "write": 1,
      "if_owner": 1
