{
  "doctype": "DocType",
  "name": "Repair Estimate",
  "module": "Service Planning",
  "engine": "InnoDB",
  "custom": 0,
  "istable": 0,
  "editable_grid": 1,
  "field_order": [
    "section_reference",
    "repair_order",
    "customer",
    "instrument_profile",
    "inspection_reference",
    "estimated_completion",
    "column_break_details",
    "instrument_family",
    "instrument_serial",
    "condition_score",
    "rush_service",
    "eta_days",
    "estimator_notes",
    "pad_map_artifact",
