{
  "doctype": "DocType",
  "name": "Instrument Profile",
  "module": "Instrument Profile",
  "custom": 0,
  "fields": [
    {
      "fieldname": "brand",
      "label": "Brand",
      "fieldtype": "Link",
      "options": "Brand"
    },
    {
      "fieldname": "clarinet_pad_map",
      "label": "Pad Map View",
      "fieldtype": "HTML",
      "depends_on": "eval:doc.instrument_category=='Clarinet'"
    },
    {
      "fieldname": "client_profile",
      "label": "Client Profile",
      "fieldtype": "Link",
      "options": "Client Profile"
    },
    {
      "fieldname": "instrument_category",
      "label": "Instrument Category",
      "fieldtype": "Select",
      "options": "Clarinet\nSaxophone\nFlute"
    },
    {
      "fieldname": "last_service_date",
      "label": "Last Service Date",
      "fieldtype": "Date",
      "read_only": 1
    },
    {
      "fieldname": "model",
      "label": "Model",
      "fieldtype": "Data"
    },
    {
      "fieldname": "owner",
      "label": "Owner",
      "fieldtype": "Link",
      "options": "Customer"
    },
    {
      "fieldname": "photo",
      "label": "Photo",
      "fieldtype": "Attach Image"
    },
    {
      "fieldname": "published",
      "label": "Published",
      "fieldtype": "Check",
      "default": 1
    },
    {
      "fieldname": "qr_code_svg",
      "label": "QR Code",
      "fieldtype": "HTML",
      "read_only": 1
    },
    {
      "fieldname": "route",
      "label": "Route",
      "fieldtype": "Data",
      "reqd": 1
    },
    {
      "fieldname": "serial_number",
      "label": "Serial Number",
      "fieldtype": "Data",
      "unique": 1,
      "reqd": 1
    },
    {
      "fieldname": "wellness_score",
      "label": "Wellness Score",
      "fieldtype": "Int",
      "default": 0
    }
  ],
  "permissions": [
    {
      "role": "System Manager",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1
    }
  ],
  "editable_grid": 1,
  "istable": 0,
  "standard": 1
}
