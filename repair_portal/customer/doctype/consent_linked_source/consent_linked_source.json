{
  "doctype": "DocType",
  "name": "Consent Linked Source",
  "module": "Customer",
  "engine": "InnoDB",
  "istable": 1,
  "fields": [
    { "fieldname": "enabled", "label": "Enabled", "fieldtype": "Check", "default": "1", "in_list_view": 1, "reqd": 1 },
    { "fieldname": "label", "label": "Link Field Label", "fieldtype": "Data", "reqd": 1, "in_list_view": 1 },
    { "fieldname": "fieldname", "label": "Link Fieldname (snake_case)", "fieldtype": "Data", "reqd": 1, "in_list_view": 1,
      "description": "e.g., instrument_profile" },
    { "fieldname": "source_doctype", "label": "Source DocType", "fieldtype": "Link", "options": "DocType", "reqd": 1, "in_list_view": 1 },
    { "fieldname": "insert_after", "label": "Insert After (fieldname)", "fieldtype": "Data", "default": "consent_template",
      "description": "Fieldname on Consent Form after which this Link will be inserted" },

    { "fieldname": "cb1", "fieldtype": "Column Break" },

    { "fieldname": "reqd", "label": "Required", "fieldtype": "Check", "default": "0" },
    { "fieldname": "read_only", "label": "Read Only", "fieldtype": "Check", "default": "0" },
    { "fieldname": "hidden", "label": "Hidden", "fieldtype": "Check", "default": "0" },
    { "fieldname": "in_list_view", "label": "In List View", "fieldtype": "Check", "default": "0" },
    { "fieldname": "permlevel", "label": "Permlevel", "fieldtype": "Int", "default": "0" },
    { "fieldname": "depends_on", "label": "Depends On (eval)", "fieldtype": "Data" },

    { "fieldname": "sb_desc", "label": "Notes", "fieldtype": "Section Break" },
    { "fieldname": "description", "label": "Description", "fieldtype": "Small Text" }
  ],
  "permissions": [
    { "role": "System Manager", "read": 1, "write": 1, "create": 1, "delete": 1 }
  ]
}
