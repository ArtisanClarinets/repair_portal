      "options": "Draft\nSubmitted",
      "default": "Draft",
      "read_only": 1,
      "in_list_view": 1
    },
    {
      "fieldname": "linked_loaner",
      "label": "Loaner Instrument",
      "fieldtype": "Link",
      "options": "Loaner Instrument",
      "reqd": 1,
      "in_list_view": 1,
      "search_index": 1
    },
    {
      "fieldname": "linked_intake",
      "label": "Clarinet Intake",
      "fieldtype": "Link",
      "options": "Clarinet Intake",
      "search_index": 1
    },
    {
      "fieldname": "agreement_template",
      "label": "Consent Template",
      "fieldtype": "Link",
      "options": "Consent Template"
    },
    {
      "fieldname": "terms_ack",
      "label": "Borrower Agrees to Terms",
      "fieldtype": "Check",
      "default": "0",
      "reqd": 1
    },
    {
      "fieldname": "section_signatures",
      "fieldtype": "Section Break",
      "label": "Signatures"
    },
    {
      "fieldname": "borrower_signature",
      "label": "Borrower Signature",
      "fieldtype": "Signature",
      "reqd": 1
    },
    {
      "fieldname": "staff_signature",
      "label": "Staff Signature",
      "fieldtype": "Signature",
      "reqd": 1
    },
    {
      "fieldname": "section_media",
      "fieldtype": "Section Break",
      "label": "Photos"
