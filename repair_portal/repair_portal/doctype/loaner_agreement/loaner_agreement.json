  "is_submittable": 1,
  "allow_rename": 0,
  "allow_copy": 0,
  "allow_import": 0,
  "field_order": [
    "status",
    "linked_loaner",
    "linked_intake",
    "agreement_template",
    "terms_ack",
    "section_signatures",
    "borrower_signature",
    "staff_signature",
    "section_media",
    "photos"
