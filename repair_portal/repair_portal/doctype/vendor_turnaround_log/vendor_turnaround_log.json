{
 "doctype": "DocType",
 "name": "Vendor Turnaround Log",
 "module": "Repair Portal",
 "custom": 0,
 "istable": 0,
 "track_changes": 1,
 "engine": "InnoDB",
 "naming_rule": "By Field",
 "autoname": "naming_series:",
 "allow_import": 1,
 "fields": [
  {
   "fieldname": "naming_series",
   "fieldtype": "Select",
   "label": "Naming Series",
   "options": "VEND-TURN-.#####",
   "reqd": 1,
   "default": "VEND-TURN-.#####"
  },
  {
   "fieldname": "vendor",
   "fieldtype": "Link",
   "label": "Vendor",
   "options": "Supplier",
   "reqd": 1
  },
  {
   "fieldname": "service_type",
   "fieldtype": "Select",
   "label": "Service Type",
   "options": "Plating\nPad Kit\nOther",
   "reqd": 1
  },
  {
   "fieldname": "avg_days",
   "fieldtype": "Int",
   "label": "Average Days"
  },
  {
   "fieldname": "last_order_date",
   "fieldtype": "Date",
   "label": "Last Order Date"
  },
  {
   "fieldname": "notes",
   "fieldtype": "Small Text",
   "label": "Notes"
  }
 ],
 "field_order": [
  "naming_series",
  "vendor",
  "service_type",
  "avg_days",
  "last_order_date",
  "notes"
 ],
 "permissions": [
  {
   "role": "Owner/Admin",
   "read": 1,
   "write": 1,
   "create": 1,
   "delete": 1
  },
  {
   "role": "Inventory",
   "read": 1,
   "write": 1,
   "create": 1
  },
  {
   "role": "Front Desk",
   "read": 1
  }
 ]
}
