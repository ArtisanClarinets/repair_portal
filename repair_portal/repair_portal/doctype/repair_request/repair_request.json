{
 "doctype": "DocType",
 "name": "Repair Request",
 "module": "Repair Portal",
 "custom": 0,
 "istable": 0,
 "track_changes": 1,
 "engine": "InnoDB",
 "naming_rule": "By Field",
 "autoname": "naming_series:",
 "allow_import": 1,
 "is_submittable": 0,
 "allow_guest_to_view": 0,
 "fields": [
  {
   "fieldname": "naming_series",
   "fieldtype": "Select",
   "label": "Naming Series",
   "options": "REP-REQ-.YYYY.-.#####",
   "reqd": 1,
   "default": "REP-REQ-.YYYY.-.#####"
  },
  {
   "fieldname": "customer",
   "fieldtype": "Link",
   "label": "Customer",
   "options": "Customer",
   "reqd": 1
  },
  {
   "fieldname": "instrument",
   "fieldtype": "Link",
   "label": "Instrument",
   "options": "Instrument",
   "reqd": 1
  },
  {
   "fieldname": "requested_services",
   "fieldtype": "Small Text",
   "label": "Requested Services",
   "reqd": 1
  },
  {
   "fieldname": "preferred_carrier",
   "fieldtype": "Select",
   "label": "Preferred Carrier",
   "options": "UPS\nUSPS\nFedEx\nOther"
  },
  {
   "fieldname": "insurance_value",
   "fieldtype": "Currency",
   "label": "Insurance Value"
  },
  {
   "fieldname": "portal_token",
   "fieldtype": "Data",
   "label": "Portal Token",
   "read_only": 1
  }
 ],
 "field_order": [
  "naming_series",
  "customer",
  "instrument",
  "requested_services",
  "preferred_carrier",
  "insurance_value",
  "portal_token"
 ],
 "permissions": [
  {
   "role": "Owner/Admin",
   "read": 1,
   "write": 1,
   "create": 1,
   "delete": 1
  },
  {
   "role": "Front Desk",
   "read": 1,
   "create": 1,
   "write": 1
  },
  {
   "role": "Repair Technician",
   "read": 1
  },
  {
   "role": "Customer",
   "read": 1,
   "create": 1
  }
 ]
}
