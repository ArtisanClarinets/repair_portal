{
 "doctype": "DocType",
 "name": "Rental Contract",
 "module": "Repair Portal",
 "custom": 0,
 "istable": 0,
 "track_changes": 1,
 "engine": "InnoDB",
 "naming_rule": "By Field",
 "autoname": "naming_series:",
 "allow_import": 1,
 "fields": [
  {
   "fieldname": "naming_series",
   "fieldtype": "Select",
   "label": "Naming Series",
   "options": "RENT-.YYYY.-.#####",
   "reqd": 1,
   "default": "RENT-.YYYY.-.#####"
  },
  {
   "fieldname": "customer",
   "fieldtype": "Link",
   "label": "Customer",
   "options": "Customer",
   "reqd": 1
  },
  {
   "fieldname": "instrument",
   "fieldtype": "Link",
   "label": "Instrument Item",
   "options": "Item",
   "reqd": 1
  },
  {
   "fieldname": "serial_no",
   "fieldtype": "Data",
   "label": "Serial No",
   "reqd": 1
  },
  {
   "fieldname": "start_date",
   "fieldtype": "Date",
   "label": "Start Date",
   "reqd": 1
  },
  {
   "fieldname": "due_date",
   "fieldtype": "Date",
   "label": "Due Date",
   "reqd": 1
  },
  {
   "fieldname": "billing_plan",
   "fieldtype": "Select",
   "label": "Billing Plan",
   "options": "Monthly\nQuarterly\nYearly",
   "reqd": 1
  },
  {
   "fieldname": "damage_waiver",
   "fieldtype": "Check",
   "label": "Damage Waiver",
   "default": "0"
  },
  {
   "fieldname": "status",
   "fieldtype": "Select",
   "label": "Status",
   "options": "Draft\nActive\nOverdue\nReturned",
   "default": "Draft"
  },
  {
   "fieldname": "school_account",
   "fieldtype": "Link",
   "label": "School Account",
   "options": "Customer"
  }
 ],
 "field_order": [
  "naming_series",
  "customer",
  "instrument",
  "serial_no",
  "start_date",
  "due_date",
  "billing_plan",
  "damage_waiver",
  "status",
  "school_account"
 ],
 "permissions": [
  {
   "role": "Owner/Admin",
   "read": 1,
   "write": 1,
   "create": 1,
   "delete": 1
  },
  {
   "role": "Front Desk",
   "read": 1,
   "write": 1,
   "create": 1
  },
  {
   "role": "Accounting",
   "read": 1,
   "write": 1
  },
  {
   "role": "School/Teacher",
   "read": 1
  }
 ]
}
