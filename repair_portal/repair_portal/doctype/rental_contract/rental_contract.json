  {
   "fieldname": "customer",
   "fieldtype": "Link",
   "label": "Customer",
   "options": "Customer",
   "reqd": 1
  },
  {
   "fieldname": "company",
   "fieldtype": "Link",
   "label": "Company",
   "options": "Company",
   "reqd": 1
  },
  {
   "fieldname": "instrument",
   "fieldtype": "Link",
   "label": "Instrument Item",
   "options": "Item",
   "reqd": 1
  },
  {
   "fieldname": "serial_no",
   "fieldtype": "Data",
   "label": "Serial No",
   "reqd": 1
  },
  {
   "fieldname": "warehouse",
   "fieldtype": "Link",
   "label": "Warehouse",
   "options": "Warehouse"
  },
  {
   "fieldname": "start_date",
   "fieldtype": "Date",
   "label": "Start Date",
   "reqd": 1
