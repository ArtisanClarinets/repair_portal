 "field_order": [
  "naming_series",
  "customer",
  "company",
  "school_account",
  "instrument",
  "serial_no",
  "warehouse",
  "billing_item",
  "damage_item",
  "start_date",
  "due_date",
  "return_date",
  "billing_plan",
  "damage_waiver",
  "status",
  "workflow_state",
  "subscription",
  "delivery_note",
  "damage_invoice",
  "inspection_section",
  "inspection_notes",
  "inspection_findings",
