{
 "doctype": "DocType",
 "name": "Player Profile",
 "module": "Repair Portal",
 "custom": 0,
 "istable": 0,
 "editable_grid": 0,
 "track_changes": 1,
 "engine": "InnoDB",
 "naming_rule": "By Field",
 "autoname": "naming_series:",
 "allow_import": 1,
 "is_submittable": 0,
 "fields": [
  {
   "fieldname": "naming_series",
   "fieldtype": "Select",
   "label": "Naming Series",
   "options": "PLYR-.YYYY.-.#####",
   "reqd": 1,
   "default": "PLYR-.YYYY.-.#####"
  },
  {
   "fieldname": "customer",
   "fieldtype": "Link",
   "label": "Customer",
   "options": "Customer",
   "reqd": 1
  },
  {
   "fieldname": "primary_instrument",
   "fieldtype": "Link",
   "label": "Primary Instrument",
   "options": "Instrument"
  },
  {
   "fieldname": "skill_level",
   "fieldtype": "Select",
   "label": "Skill Level",
   "options": "Student\nHobbyist\nSemi-Pro\nPro",
   "reqd": 1
  },
  {
   "fieldname": "preferences",
   "fieldtype": "Long Text",
   "label": "Preferences"
  },
  {
   "fieldname": "marketing_consent",
   "fieldtype": "Check",
   "label": "Marketing Consent",
   "default": "0"
  },
  {
   "fieldname": "consent_timestamp",
   "fieldtype": "Datetime",
   "label": "Consent Timestamp"
  }
 ],
 "field_order": [
  "naming_series",
  "customer",
  "primary_instrument",
  "skill_level",
  "preferences",
  "marketing_consent",
  "consent_timestamp"
 ],
 "permissions": [
  {
   "role": "Owner/Admin",
   "read": 1,
   "write": 1,
   "create": 1,
   "delete": 1
  },
  {
   "role": "Front Desk",
   "read": 1,
   "create": 1,
   "write": 1
  },
  {
   "role": "Repair Technician",
   "read": 1,
   "write": 1
  },
  {
   "role": "Ecommerce/Marketing",
   "read": 1,
   "write": 1
  },
  {
   "role": "Customer",
   "read": 1
  }
 ]
}
