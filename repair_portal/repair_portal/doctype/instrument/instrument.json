{
  "doctype": "DocType",
  "name": "Instrument",
  "module": "Repair Portal",
  "custom": 0,
  "istable": 0,
  "editable_grid": 0,
  "track_changes": 1,
  "engine": "InnoDB",
  "naming_rule": "By Field",
  "autoname": "naming_series:",
  "allow_import": 1,
  "is_submittable": 0,
  "show_name_in_global_search": 1,
  "fields": [
    {
      "fieldname": "naming_series",
      "fieldtype": "Select",
      "label": "Naming Series",
      "options": "INS-.YYYY.-.#####",
      "reqd": 1,
      "default": "INS-.YYYY.-.#####"
    },
    {
      "fieldname": "customer",
      "fieldtype": "Link",
      "label": "Customer",
      "options": "Customer",
      "reqd": 1
    },
    {
      "fieldname": "serial_no",
      "fieldtype": "Data",
      "label": "Serial No",
      "unique": 1,
      "reqd": 1
    },
    {
      "fieldname": "make",
      "fieldtype": "Data",
      "label": "Make",
      "reqd": 1
    },
    {
      "fieldname": "model",
      "fieldtype": "Data",
      "label": "Model",
      "reqd": 1
    },
    {
      "fieldname": "family",
      "fieldtype": "Select",
      "label": "Family",
      "options": "Bb\nA\nEb\nBass",
      "reqd": 1
    },
    {
      "fieldname": "finish",
      "fieldtype": "Select",
      "label": "Finish",
      "options": "Silver\nNickel\nGold\nOther",
      "reqd": 1
    },
    {
      "fieldname": "purchase_date",
      "fieldtype": "Date",
      "label": "Purchase Date"
    },
    {
      "fieldname": "warranty_end",
      "fieldtype": "Date",
      "label": "Warranty End"
    },
    {
      "fieldname": "notes",
      "fieldtype": "Small Text",
      "label": "Notes"
    },
    {
      "fieldname": "portal_visible",
      "fieldtype": "Check",
      "label": "Portal Visible",
      "default": "0"
    },
    {
      "fieldname": "photos",
      "fieldtype": "Table",
      "label": "Photos",
      "options": "Instrument Photo"
    },
    {
      "fieldname": "barcode",
      "fieldtype": "Data",
      "label": "Barcode",
      "read_only": 1
    }
  ],
  "field_order": [
    "naming_series",
    "customer",
    "serial_no",
    "make",
    "model",
    "family",
    "finish",
    "purchase_date",
    "warranty_end",
    "notes",
    "portal_visible",
    "photos",
    "barcode"
  ],
  "permissions": [
    {
      "role": "Owner/Admin",
      "read": 1,
      "write": 1,
      "create": 1,
      "delete": 1
    },
    {
      "role": "Front Desk",
      "read": 1,
      "write": 1,
      "create": 1
    },
    {
      "role": "Repair Technician",
      "read": 1,
      "write": 1
    },
    {
      "role": "Customer",
      "read": 1
    }
  ]
}