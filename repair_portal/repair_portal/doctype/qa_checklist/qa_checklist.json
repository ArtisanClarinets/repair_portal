{
 "doctype": "DocType",
 "name": "QA Checklist",
 "module": "Repair Portal",
 "custom": 0,
 "istable": 0,
 "track_changes": 1,
 "engine": "InnoDB",
 "naming_rule": "By Field",
 "autoname": "naming_series:",
 "allow_import": 1,
 "fields": [
  {
   "fieldname": "naming_series",
   "fieldtype": "Select",
   "label": "Naming Series",
   "options": "QA-CHK-.YYYY.-.#####",
   "reqd": 1,
   "default": "QA-CHK-.YYYY.-.#####"
  },
  {
   "fieldname": "repair_order",
   "fieldtype": "Link",
   "label": "Repair Order",
   "options": "Repair Order",
   "reqd": 1
  },
  {
   "fieldname": "steps",
   "fieldtype": "Table",
   "label": "Steps",
   "options": "QA Step"
  },
  {
   "fieldname": "passed",
   "fieldtype": "Check",
   "label": "Passed",
   "default": "0"
  },
  {
   "fieldname": "technician",
   "fieldtype": "Link",
   "label": "Technician",
   "options": "User"
  },
  {
   "fieldname": "signoff_datetime",
   "fieldtype": "Datetime",
   "label": "Signoff Datetime"
  }
 ],
 "field_order": [
  "naming_series",
  "repair_order",
  "steps",
  "passed",
  "technician",
  "signoff_datetime"
 ],
 "permissions": [
  {
   "role": "Owner/Admin",
   "read": 1,
   "write": 1,
   "create": 1,
   "delete": 1
  },
  {
   "role": "Repair Technician",
   "read": 1,
   "write": 1,
   "create": 1
  },
  {
   "role": "Front Desk",
   "read": 1
  },
  {
   "role": "Customer",
   "read": 1
  }
 ]
}
