{
  "doctype": "DocType",
  "name": "Setup Template",
  "module": "Instrument Setup",
  "engine": "InnoDB",
  "track_changes": 1,
  "field_order": [
    "template_name",
    "clarinet_model",
    "pad_map",
    "default_operations",
    "checklist_items",
    "template_tasks"
  ],
  "fields": [
    {
      "fieldname": "template_name",
      "fieldtype": "Data",
      "label": "Template Name",
      "unique": 1,
      "in_list_view": 1,
      "in_standard_filter": 1,
      "set_only_once": 1,
      "hidden": 1
    },
    {
      "fieldname": "clarinet_model",
      "fieldtype": "Link",
      "label": "Instrument Model",
      "options": "Instrument Model",
      "in_standard_filter": 1,
      "description": "Model this template is designed for."
    },
    {
      "fieldname": "pad_map",
      "fieldtype": "Link",
      "label": "Pad Map",
      "options": "Clarinet Pad Map"
    },
    {
      "fieldname": "default_operations",
      "fieldtype": "Table",
      "label": "Default Operations",
      "options": "Clarinet Setup Operation",
      "description": "Operations to pre-populate on a Clarinet Initial Setup."
    },
    {
      "fieldname": "checklist_items",
      "fieldtype": "Table",
      "label": "Checklist Items",
      "options": "Setup Checklist Item",
      "description": "Items to check during setup (each may have status, notes)."
    },
    {
      "fieldname": "template_tasks",
      "fieldtype": "Table",
      "label": "Template Tasks",
      "options": "Clarinet Template Task",
      "description": "Task rows used to instantiate Clarinet Setup Task documents."
    }
  ],
  "permissions": [
    { "role": "Technician", "read": 1, "write": 1, "create": 1 },
    { "role": "Service Manager", "read": 1, "write": 1, "create": 1 },
    { "role": "System Manager", "read": 1, "write": 1, "create": 1, "delete": 1 }
  ],
  "sort_field": "modified",
  "sort_order": "DESC"
}
