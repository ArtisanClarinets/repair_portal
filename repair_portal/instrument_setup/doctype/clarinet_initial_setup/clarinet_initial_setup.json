{
  "doctype": "DocType",
  "name": "Clarinet Initial Setup",
  "module": "Instrument Setup",
  "engine": "InnoDB",
  "autoname": "format: CSU-{YY}{MM}-{####}",
  "track_changes": 1,
  "track_seen": 1,
  "track_views": 1,
  "field_order": [
    "instrument_information_section",
    "status",
    "serial_no",
    "instrument",
    "instrument_profile",
    "clarinet_type",
    "model",
    "work_photos",
    "column_break_ezgh",
    "setup_date",
    "clarinet_initial_setup_id",
    "progress",
    "intake",
    "inspection",
    "setup_template",
    "labor_hours",
    "technician",
    "setup_logs",
    "setup_logs_table",
    "setup_checklist_section",
    "checklist",
    "materials_used",
    "operations_performed",
    "technical_tags",
    "amended_from"
  ],
  "fields": [
    { "fieldname": "instrument_profile", "fieldtype": "Link", "label": "Instrument Profile", "options": "Instrument Profile", "hidden": 1, "read_only": 1 },
    { "fieldname": "clarinet_initial_setup_id", "fieldtype": "Data", "label": "Setup ID", "hidden": 1, "read_only": 1, "unique": 1 },
    { "fieldname": "serial_no", "fieldtype": "Link", "label": "Serial No", "options": "Serial No", "fetch_from": "instrument.serial_no", "fetch_if_empty": 1 },
    { "fieldname": "intake", "fieldtype": "Link", "label": "Intake", "options": "Clarinet Intake" },
    { "fieldname": "instrument", "fieldtype": "Link", "label": "Instrument", "options": "Instrument", "reqd": 1 },
    { "fieldname": "clarinet_type", "fieldtype": "Select", "label": "Type of Clarinet", "options": "B♭ Clarinet\nA Clarinet\nE♭ Clarinet\nBass Clarinet\nAlto Clarinet\nContrabass Clarinet\nOther" },
    { "fieldname": "model", "fieldtype": "Data", "label": "Model", "read_only": 1, "fetch_from": "instrument.model", "fetch_if_empty": 1 },
    { "fieldname": "inspection", "fieldtype": "Link", "label": "Inspection", "options": "Instrument Inspection", "read_only": 1, "fetch_from": "instrument_profile.linked_inspection", "fetch_if_empty": 1 },
    { "fieldname": "setup_template", "fieldtype": "Link", "label": "Setup Template", "options": "Setup Template" },
    { "fieldname": "setup_date", "fieldtype": "Date", "label": "Setup Date" },
    { "fieldname": "technician", "fieldtype": "Link", "label": "Technician", "options": "User" },

    { "fieldname": "progress", "fieldtype": "Percent", "label": "Progress", "read_only": 1, "in_list_view": 1 },

    { "fieldname": "checklist", "fieldtype": "Table", "label": "Checklist", "options": "Setup Checklist Item" },

    { "fieldname": "materials_used", "fieldtype": "Table", "label": "Materials Used", "options": "Setup Material Log" },

    { "fieldname": "operations_performed", "fieldtype": "Table", "label": "Operations Performed", "options": "Clarinet Setup Operation" },

    { "fieldname": "work_photos", "fieldtype": "Attach Image", "label": "Work Photos" },
    { "fieldname": "technical_tags", "fieldtype": "Text Editor", "label": "Technical Tags/Notes" },
    { "fieldname": "labor_hours", "fieldtype": "Float", "label": "Labor Hours" },

    { "fieldname": "status", "fieldtype": "Select", "label": "Status", "options": "Open\nPending\nPass\nFail", "in_list_view": 1, "in_standard_filter": 1 },

    { "fieldname": "amended_from", "fieldtype": "Link", "label": "Amended From", "options": "Clarinet Initial Setup", "no_copy": 1, "read_only": 1, "print_hide": 1, "search_index": 1 },

    { "fieldname": "instrument_information_section", "fieldtype": "Section Break", "label": "Instrument Information" },
    { "fieldname": "column_break_ezgh", "fieldtype": "Column Break" },

    { "fieldname": "setup_logs", "fieldtype": "Section Break", "label": "Setup Logs" },
    { "fieldname": "setup_logs_table", "fieldtype": "Table", "label": "Setup Logs", "options": "Clarinet Setup Log" },

    { "fieldname": "setup_checklist_section", "fieldtype": "Section Break", "label": "Setup Checklist" }
  ],
  "permissions": [
    { "role": "Technician", "create": 1, "read": 1, "write": 1 },
    { "role": "Service Manager", "create": 1, "read": 1, "write": 1, "delete": 1, "submit": 1 },
    { "role": "System Manager", "create": 1, "read": 1, "write": 1, "delete": 1, "submit": 1 },
    { "role": "Customer", "create": 1, "read": 1, "if_owner": 1 }
  ],
  "sort_field": "modified",
  "sort_order": "DESC"
}
